{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "max-len": ["error", { "code": 110, "tabWidth": 2 }]
  }
}
